{% include 'header.twig'%}
    <body>
        {% include 'parts/navigation.twig'%}
        <main>
            <div class="container-fluid">
                <div class="row">
                    <div class="main">
                        <div class="mainFrame">
                            <h1>Our Projects</h1>
                        </div>
                    </div>
                    {% for project in projects %}
                    <div class="col-sm-12 projects">
                        <div class="col-sm-4 thumb">
                            <div class="thumbnail">
                                <div>
                                {% for picture in project['project_pics'] %}
                                    {% if picture['type'] == 'thumb' %}
                                    <a class="example-image-link" href={{picture['url']}} data-lightbox={{picture['data_light_box']}} data-title={{picture['data_title']}}>
                                        <img class="example-image" src={{picture['url']}} alt=""/>
                                    </a>
                                    {% endif %}
                                    {% if picture['type'] == 'normal' %}
                                    <a class="example-image-link" href={{picture['url']}} data-lightbox={{picture['data_light_box']}} data-title={{picture['data_title']}}>
                                        <img class="example-image hidden" src={{picture['url']}} alt=""/>
                                    </a>
                                    {% endif %}
                                 {% endfor %}
                                </div>
                                <div class="caption">
                                    <p> {{ project['title'] }} </p>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-8 text">
                            <h3>{{ project['title'] }}</h3>
                            <p>{{ project['about'] }}</p>
                        </div>
                        </div>
                        {% endfor %}
                    <div class="col-xs-12">
                        <div class="col-xs-4 clients stig" onclick="newTabOpen('http://www.stigco.com/')">
                        <img src="/front-end/resources/img/logo/STIG.svg" alt="" >
                        </div>
                        <div class="col-xs-4 clients mizan" onclick="newTabOpen('http://mizanunderwear.com/')">
                            <img src="/front-end/resources/img/logo/MIZAN.svg" alt="" >
                        </div>
                        <div class="col-xs-4 clients barbosa" onclick="newTabOpen('http://barbosa.rs/')">
                            <img src="/front-end/resources/img/logo/BARBOSA.png" alt="" >
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <div class="map">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6 info">
                        <ul>    
                            <li>
                                <span>
                                    <img src="/front-end/resources/img/navicons/placeholder.svg" alt="">
                                    Novi Pazar
                                </span>
                            </li>
                            <li>
                                <span>
                                    <img src="/front-end/resources/img/navicons/envelope.svg" alt="">
                                    info@hcg.rs
                                </span>
                            </li>
                            <li>
                                <span>
                                    <img src="/front-end/resources/img/navicons/phone-call.svg" alt="">
                                    +381 63. 264 077
                                </span>
                            </li>
                            <li>
                                <span>
                                    <img src="/front-end/resources/img/navicons/phone-call.svg" alt="">
                                    +381 69. 277 3798
                                </span>
                            </li>
                        </ul>
                    </div>
                    <div class="col-sm-6 googleMap">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d1247.7673663570217!2d20.51851508570493!3d43.14413390272474!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2smk!4v1485278999053" width="100%" height="35%" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>
                </div>
            </div>
        </div>
        <article>
            <div class="container-fluid">
                <div class="row toExample">
                    <div class="col-sm-12 textExample">
                        <h3>With us, your visions are achivable</h3>
                    </div>
                </div>
            </div>
        </article>


         {% include 'parts/footer.twig'%}
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <script src="/front-end/resources/js/script.js"></script>
        <script src="/front-end/resources/js/lightbox.min.js"></script>
    </body>
</html>